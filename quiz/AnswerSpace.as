package {	import flash.display.MovieClip;	import flash.events.MouseEvent;	public class AnswerSpace extends MovieClip {		public function AnswerSpace() {			this.addEventListener(MouseEvent.MOUSE_OVER, hover);			this.addEventListener(MouseEvent.MOUSE_OUT, unhover);			this.addEventListener(MouseEvent.CLICK, clicked);			this.alpha = 0;		}		public function hover(e:MouseEvent):void {			this.alpha = .25;		}		public function unhover(e:MouseEvent):void {			this.alpha = 0;		}				public function clicked(e:MouseEvent):void {			trace(this.name+" clicked");		}	}}