package classes{	public class ProfileList {				private var First:ProfileNode = null;		public var length:int = 0;		private var isLoading:Boolean = false;		private var forceLoading:Boolean = false;				public function ProfileList() 		{			// constructor code		}				public function Add(Name:String, ID:String, ImageURL:String)		{			if (First == null) First = new ProfileNode(Name, ID, ImageURL);			else			{				var cur:ProfileNode = First;				while (cur.Next != null) cur = cur.Next;				cur.Next = new ProfileNode(Name, ID, ImageURL);			}			length++;		}		public function Get(index:int) : ProfileNode		{			if (index > length - 1) return null;						var cur:ProfileNode = First;			for (var n:int = 0; n < index; n++)			{				cur = cur.Next;			}			return cur;		}	}	}