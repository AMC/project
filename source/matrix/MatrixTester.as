package  {		import flash.display.MovieClip;	import flash.events.Event;			public class MatrixTester extends MovieClip {				private var cardBack:MovieClip;				public function MatrixTester() 		{			this.addEventListener(Event.ADDED_TO_STAGE, onAddedToStage);		}		private function onAddedToStage(e:Event)		{			this.removeEventListener(Event.ADDED_TO_STAGE, onAddedToStage);						cardBack = new CardBack();			addChild(cardBack);			cardBack.x = 200;			cardBack.y = 200;						this.addEventListener(Event.ENTER_FRAME, onFrame);		}		private function onFrame(e:Event)		{			cardBack.rotationY += 1.0;		}	}	}