package  {	import flash.events.Event;		public class MatchGame extends Minigame	{		private var profiles:Array;		private var motion:MotionEngine;				public function MatchGame() 		{			motion = new MotionEngine();						profiles = new Array();						this.addEventListener(Event.ADDED_TO_STAGE, onAddedToStage);		}		private function onAddedToStage(e:Event)		{			this.removeEventListener(Event.ADDED_TO_STAGE, onAddedToStage);		}	}	}